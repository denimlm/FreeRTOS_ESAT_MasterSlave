#ifndef ESAT_MASTERSLAVE_ESAT_MS_TYPES_H_
#define ESAT_MASTERSLAVE_ESAT_MS_TYPES_H_

#include <stdint.h>

enum  en_MasterStates {
	INIT = 0,
	IDLE,
	PROCESSING,
	ERROR
};

enum  en_SlaveStates{
	SLEEP,
	ACTIVE,
	FAULT
};

#endif /* ESAT_MASTERSLAVE_ESAT_MS_TYPES_H_ */
